<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.qianfeng.bean.Temporary" table="temporary">
		<id name="id" column="id">
			<generator class="uuid"></generator>
		</id>
		
		<property name="phone"></property>
		<property name="brand"></property>
		<property name="series"></property>
		<property name="month"></property>
		<property name="year"></property>
		<property name="address"></property>
		<property name="mileage"></property>
		<property name="car_license_location"></property>
		<property name="vehicle_condition"></property>
		<property name="time"></property>

	</class>
</hibernate-mapping>
